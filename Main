package edu.monmouth.shapes;

public class Main {

	public static void main(String[] args) {
		Circle circle;
		Rectangle rectangle;
		Shape shape1, shape2;
		
		circle = new Circle(12);
		rectangle = new Rectangle (21, 34);
		shape1 = new Circle(90);
		shape2 = new Rectangle(2, 4);
		
		System.out.println(circle.getRadius() + " " + circle.area());
		System.out.println(rectangle.getLength() + " " + rectangle.getWidth() + " " + rectangle.area());
		System.out.println(shape1.area());
		System.out.println(shape2.area());
	} 
}
